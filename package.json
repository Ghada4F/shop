{
  "name": "shop.js",
  "version": "2.2.68",
  "description": "Ecommerce UI components and framework powered by Hanzo",
  "homepage": "http://github.com/hanzoai/shop.js",
  "bugs": {
    "url": "https://github.com/hanzoai/hanzo/issues"
  },
  "author": "Hanzo, Inc <hi@hanzo.ai>",
  "license": "BSD-3-Clause",
  "keywords": [
    "browser",
    "checkout",
    "commerce",
    "crowdfunding",
    "ecommerce",
    "el.js",
    "fulfillment",
    "hanzo",
    "orders",
    "payments",
    "preorders",
    "products",
    "referrals",
    "shipping",
    "site",
    "subscriptions"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "directories": {
    "src": "src",
    "dist": "dist"
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "github:hanzoai/shop.js"
  },
  "scripts": {
    "audit": "node_modules/.bin/source-map-explorer dist/index.umd.js",
    "prepare": "npm run build",
    "clean": "rm -rf dist",
    "build": "npm run clean && node_modules/.bin/rollup -c",
    "build:watch": "node_modules/.bin/rollup -c -w",
    "watch": "node_modules/.bin/concurrently 'npm run build:watch' 'node_modules/.bin/serve -l tcp://127.0.0.1'",
    "test": "node_modules/.bin/jest && npm run build",
    "test:coverage": "node_modules/.bin/jest --collect-coverage"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-json": "4.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "@types/jest": "25.1.3",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "babel-jest": "25.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "jest": "25.1.0",
    "rollup": "1.31.1",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-peer-deps-external": "2.2.2",
    "rollup-plugin-typescript2": "0.26.0",
    "rollup-plugin-visualizer": "3.3.1",
    "source-map-explorer": "2.3.1",
    "source-map-support": "0.5.16",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "tslib": "1.11.0",
    "typedoc": "github:TypeStrong/typedoc#5704bf5",
    "typedoc-plugin-markdown": "2.2.16",
    "typescript": "3.8.2"
  },
  "renovate": {
    "extends": [
      "config:js-lib"
    ],
    "automerge": true,
    "major": {
      "automerge": false
    },
    "automergeType": "branch"
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "commerce.js": "2.0.0",
    "classnames": "2.2.6",
    "fast-memoize": "2.5.1"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.9.4",
    "midstream": "2.0.1",
    "mobx": "5.15.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
